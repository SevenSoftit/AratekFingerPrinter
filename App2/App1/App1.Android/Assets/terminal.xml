<terminal version="2.0">
    <!--
      ANDROID_LOG_UNKNOWN = 0
      ANDROID_LOG_DEFAULT = 1
      ANDROID_LOG_VERBOSE = 2
      ANDROID_LOG_DEBUG   = 3
      ANDROID_LOG_INFO    = 4
      ANDROID_LOG_WARN    = 5
      ANDROID_LOG_ERROR   = 6
      ANDROID_LOG_FATAL   = 7
      ANDROID_LOG_SILENT  = 8
    -->
    <log
        level="5"
        save="false"
        path="/sdcard/bmlog"/>

    <device
        type="idcard"
        model="auto-detect"
        forced="false" >
        <uart
            path="/dev/ttyS0"
            baudrate="115200" />
    </device>

    <device
        type="iccard"
        model="cid1000m"
        forced="true" >
    </device>
</terminal>